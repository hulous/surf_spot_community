<% if success %><p class="alert alert-success"><%= success %></p><% end %>
<% if notice %><p class="alert alert-info"><%= notice %></p><% end %>
<% if warning %><p class="alert alert-warning"><%= warning %></p><% end %>
<% if error %><p class="alert alert-danger"><%= error %></p><% end %>
<% if alert %><p class="alert alert-danger"><%= alert %></p><% end %>
